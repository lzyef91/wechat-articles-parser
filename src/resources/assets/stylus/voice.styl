/* mpvoice */
.db
    display: block

.share_audio_context
    padding: 14px 20px 6px
    background-color: #fdfdfd
    background-color: #fcfcfc
    padding: 14px 15px 6px
    font-size: 16px
    position: relative
    outline: 0
    -webkit-tap-highlight-color: rgba(0,0,0,0)
    &:before
        content: " "
        position: absolute
        top: 0
        left: 0
        border: 1px solid #e0e0e0
        border-radius: 4px
        -moz-border-radius: 4px
        -webkit-border-radius: 4px
        width: 200%
        height: 200%
        box-sizing: border-box
        -moz-box-sizing: border-box
        -webkit-box-sizing: border-box
        -webkit-transform: scale(0.5)
        transform: scale(0.5)
        -webkit-transform-origin: 0 0
        transform-origin: 0 0

.share_audio_switch
    margin: -10px 15px 0 0
    position: relative
    z-index: 1

.share_audio_playing
    .icon_share_audio_switch
        background-image: url("data:image/gif;base64,")

.icon_share_audio_switch
    background: transparent url("data:image/png;base64,") no-repeat 0 0
    width: 42px
    height: 42px
    vertical-align: middle
    display: inline-block
    background-size: 42px auto
    overflow: hidden
    color: transparent
    &:before
        content: "播放语音"

.share_audio_info
    position: relative
    outline: 0
    -webkit-tap-highlight-color: rgba(0,0,0,0)

.flex_bd
    -webkit-box-flex: 1
    -webkit-flex: 1
    flex: 1
    word-wrap: break-word
    word-break: break-all

.share_audio_title
    display: block
    font-weight: 400
    overflow: hidden
    text-overflow: ellipsis
    display: -webkit-box
    -webkit-box-orient: vertical
    -webkit-line-clamp: 1
    min-height: 1.6em
    word-wrap: break-word

.share_audio_tips
    overflow: hidden
    text-overflow: ellipsis
    display: -webkit-box
    -webkit-box-orient: vertical
    -webkit-line-clamp: 1
    padding-bottom: 6px
    font-size: 12px
    color: #888

.share_audio_progress_wrp
    height: 2px
    margin-right: 7px
    position: relative
    outline: 0
    -webkit-tap-highlight-color: rgba(0,0,0,0)

.share_audio_progress
    height: 100%
    background-color: #ebebeb
    position: relative
    width: 100%
    padding-left: 7px
    box-sizing: initial!important

.share_audio_progress_inner
    background-color: #09bb07
    height: 100%
    position: absolute
    top: 0
    left: 0
    z-index: 1

.share_audio_progress_buffer
    position: absolute
    top: 0
    left: 0
    bottom: 0
    background-color: #d9d9d9

.share_audio_progress_loading
    position: absolute
    top: 0
    bottom: 0
    left: 0
    right: 0
    overflow: hidden
    display: none

@keyframes slidein
    0%
        -webkit-transform: translateX(-50%)
        transform: translateX(-50%)

    100%
        -webkit-transform: translateX(0)
        transform: translateX(0)

.share_audio_progress_loading
    .share_audio_progress_loading_inner
        position: absolute
        top: 0
        bottom: 0
        left: 0
        -webkit-animation: slidein 6s linear infinite normal
        animation: slidein 6s linear infinite normal
        width: 200%
        max-width: none!important
        background-image: -webkit-repeating-linear-gradient(-15deg,#d9d9d9,#d9d9d9 2px,#ebebeb 2px,#ebebeb 4px)
        background-image: repeating-linear-gradient(-15deg,#d9d9d9,#d9d9d9 2px,#ebebeb 2px,#ebebeb 4px)


.share_audio_progress_handle
    z-index: 2
    position: absolute
    width: 14px
    height: 14px
    border-radius: 50%
    -moz-border-radius: 50%
    -webkit-border-radius: 50%
    background-color: rgba(9,187,7,0.15)
    top: 50%
    margin-top: -7px
    margin-left: -3.5px
    cursor: pointer
    outline: 0
    -webkit-tap-highlight-color: rgba(0,0,0,0)
    &:before
        content: " "
        width: 8px
        height: 8px
        border-radius: 50%
        -moz-border-radius: 50%
        -webkit-border-radius: 50%
        background-color: #09bb07
        position: absolute
        left: 50%
        top: 50%
        margin-top: -4px
        margin-left: -4px

.share_audio_desc
    color: #b2b2b2
    overflow: hidden
    padding-top: 6px
    font-size: 12px
    em
        font-weight: 400
        font-style: normal

.share_audio_length_current
    float: left

.share_audio_length_total
    float: right
    &:before
        position: absolute
        left: -9999em
        content: "总时长"


/* qumusic */
.qqmusic_area
    margin: 17px 1px 16px 0
    font-weight: 400
    text-decoration: none
    font-size: 0
    line-height: 0
    text-align: left
    -ms-text-size-adjust: none
    -webkit-text-size-adjust: none
    -moz-text-size-adjust: none
    text-size-adjust: none

.appmsg_card_context
    position: relative
    background-color: #fdfdfd
    border-width: 0
    border-radius: 2px
    -moz-border-radius: 2px
    -webkit-border-radius: 2px
    &:before
        content: " "
        border: 1px solid #e2e2e2
        border-radius: 4px
        -moz-border-radius: 4px
        -webkit-border-radius: 4px
        position: absolute
        top: 0
        left: 0
        width: 200%
        height: 200%
        -webkit-transform: scale(0.5)
        transform: scale(0.5)
        -webkit-transform-origin: 0 0
        transform-origin: 0 0
        box-sizing: border-box
        -moz-box-sizing: border-box
        -webkit-box-sizing: border-box

.qqmusic_wrp
    border: 1px solid #ebebeb
    line-height: 1.6

.qqmusic_bd
    background-color: transparent
    position: relative
    overflow: hidden

.play_area
    width: auto
    height: auto
    margin-right: 15px
    float: left
    position: relative

.qqmusic_playing
    .icon_qqmusic_switch
        background-position: 0 -74px

.icon_qqmusic_switch
    position: absolute
    top: 50%
    left: 50%
    margin-top: -18.5px
    margin-left: -18.5px
    line-height: 200px
    overflow: hidden
    cursor: pointer
    width: 37px
    height: 37px
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0)
    outline: 0
    background: transparent url("data:image/png;base64,") no-repeat 0 0
    background-size: 37px auto


.qqmusic_area
    .pic_qqmusic_default
        position: absolute
        top: 50%
        left: 50%
        margin-top: -18.5px
        margin-left: -18.5px
        width: 37px
        height: 37px
        display: none

.qqmusic_thumb
    width: 68px
    height: 68px !important

.access_area
    display: block
    color: #888
    min-height: 60px
    overflow: hidden
    margin-right: 10px
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0)
    outline: 0

.qqmusic_songname
    padding: 10px 0 5px
    margin-bottom: -4px
    font-size: 16px
    color: #333

.qqmusic_singername
    font-size: 13px
    margin-right: 20px


.qqmusic_songname, .qqmusic_singername
    display: block
    width: auto
    overflow: hidden
    text-overflow: ellipsis
    white-space: nowrap
    word-wrap: normal

.qqmusic_source
    position: absolute
    right: 6px
    bottom: 6px

.qqmusic_source
    img
        width: 13px
        height: 13px
        vertical-align: top
        border: 0

